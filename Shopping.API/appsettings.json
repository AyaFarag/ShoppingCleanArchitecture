{
  "ConnectionStrings": {
    "DefaultConnection": "data source=.;initial catalog=ShoppingDB;integrated security=True;MultipleActiveResultSets=True;TrustServerCertificate=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "SecurityKey": "YourVeryStrongSecurityKeyForJWT", // Replace with a strong secret key
    "Issuer": "YourIssuer",
    "Audience": "YourAudience",
    "ExpiryInMinutes": 60
  },
  "AllowedHosts": "*"
}
